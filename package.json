{
  "name": "retailedge-proxy",
  "version": "1.0.0",
  "description": "CORS proxy server for RetailEdge Pro social sentiment APIs",
  "main": "proxy-server.js",
  "scripts": {
    "start": "node proxy-server.js",
    "proxy": "node proxy-server.js",
    "dev": "nodemon proxy-server.js",
    "tournament": "node tournament-server.js",
    "build:vendor": "node build/vendor.js",
    "build:app": "node build/build.js",
    "build:css": "tailwindcss -c tailwind.config.js -i ./src/input.css -o ./dist/tailwind.css --minify",
    "build": "python3 --version && npm rebuild sqlite3 --build-from-source || echo 'Using pre-built dist files'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "lightweight-charts": "4.2.0",
    "node-fetch": "^2.7.0",
    "node-schedule": "^2.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "nodemon": "^3.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "^3.4.17"
  }
}
